# setup up web proxy

export http_proxy=http://web-proxy.cce.hp.com:8080
export https_proxy=https://web-proxy.cce.hp.com:8080

# install jpeg lib
sudo -E apt-get install libjpeg-dev


# install PIL
sudo -E apt-get install python-imaging

# install python-dev
sudo -E apt-get install python-dev

# put xuxc_site_index.html to root@zhongchuan-server:/data/home/website2/htdocs/siteadmin/template/

# access my page from http://zhongchuan-server/siteadmin/xuxc_index.php


